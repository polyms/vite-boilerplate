{
  "name": "@polyms/vite-electron-boilerplate",
  "private": true,
  "version": "0.1.1",
  "main": "dist/electron/main.js",
  "description": "Vite Electron boilerplate",
  "author": {
    "name": "Polyms",
    "email": "admin@polyms.app"
  },
  "scripts": {
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix",
    "lint:format": "prettier --loglevel warn --write './**/*.{js,jsx,ts,tsx,scss,md}' ",
    "lint": "yarn lint:format && yarn lint:fix",
    "postinstall": "electron-builder install-app-deps && node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
    "dev": "vite --host",
    "build": "tsc && vite build && electron-builder",
    "publish": "electron-builder -p always",
    "preview": "vite preview"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-regular-svg-icons": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@tanstack/react-query": "^4.24.10",
    "@tanstack/react-table": "^8.7.9",
    "axios": "^1.3.4",
    "classnames": "^2.3.2",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.3",
    "react-router": "^6.8.2",
    "react-router-dom": "^6.8.2",
    "zustand": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^18.14.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^3.1.0",
    "bootstrap": "^5.2.3",
    "electron": "^23.1.1",
    "electron-builder": "^23.6.0",
    "electron-log": "^4.4.8",
    "electron-updater": "^5.3.0",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "sass": "^1.58.3",
    "stylelint": "^15.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "typescript": "4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-svgr": "^2.4.0"
  },
  "optionalDependencies": {
    "@popperjs/core": "^2.11.6"
  },
  "packageManager": "yarn@3.2.2",
  "dependenciesMeta": {
    "bootstrap@5.2.3": {
      "unplugged": true
    }
  },
  "keywords": [
    "vite",
    "plugin",
    "electron",
    "quick-start"
  ]
}
