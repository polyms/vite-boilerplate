{
  "name": "@polyms/vite-electron-boilerplate",
  "private": true,
  "version": "0.1.1",
  "main": "dist/electron/main.js",
  "description": "Vite Electron boilerplate",
  "author": {
    "name": "Polyms",
    "email": "admin@polyms.app"
  },
  "scripts": {
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix",
    "lint:format": "prettier --loglevel warn --write './**/*.{js,jsx,ts,tsx,scss,md}' ",
    "lint": "yarn lint:format && yarn lint:fix",
    "postinstall": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "dev": "vite --host",
    "build": "tsc && vite build && electron-builder",
    "publish": "electron-builder -p always",
    "preview": "vite preview"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-regular-svg-icons": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@popperjs/core": "^2.11.6",
    "@tanstack/react-query": "^4.1.0",
    "@tanstack/react-table": "^8.5.11",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "electron-log": "^4.4.8",
    "electron-updater": "^5.2.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.34.1",
    "react-query": "^3.39.2",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "zustand": "^4.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.7.2",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vitejs/plugin-react": "^2.0.1",
    "@vitest/ui": "^0.21.1",
    "bootstrap": "^5.2.0",
    "c8": "^7.12.0",
    "electron": "^20.0.3",
    "electron-builder": "^23.3.3",
    "eslint": "^8.21.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.6.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "sass": "^1.54.4",
    "stylelint": "^14.10.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "typescript": "4.7.4",
    "vite": "^3.0.7",
    "vite-plugin-electron": "^0.9.0",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-svgr": "^2.2.1",
    "vitest": "^0.21.1"
  },
  "packageManager": "yarn@3.2.2",
  "dependenciesMeta": {
    "bootstrap@5.2.0": {
      "unplugged": true
    }
  },
  "keywords": [
    "vite",
    "plugin",
    "electron",
    "quick-start"
  ]
}
